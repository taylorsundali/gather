<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<table class="listing" summary="User list">
	<tr class="header">
		<th>&nbsp;</th>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
	</tr>
	<% @users.each do |x| %>
	<tr>
		<td><%= x.first_name %></td>
		<td><%= x.last_name %></td>
		<td><%= x.email %></td>
	</tr>
	<% end %>
</table>